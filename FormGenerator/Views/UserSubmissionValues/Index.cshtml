@model IEnumerable<FormGenerator.Models.UserSubmissionValue>


@Styles.Render("~/Content/css/css.css")

@Styles.Render("~/Content/lib/bootstrap/dist/css/bootstrap.min.css")
<div class="test">
    <div class="registration-form">
        <form asp-action="Create">
            @foreach (var item in Model)
            {
                <div class="form-group">

                    @FormGenerator.Controllers.UserSubmissionValuesController.GetFieldCaption(item.FieldId)

                    <input value="@item.SubmittedValue" type="text" class="form-control item" id="username" readonly l="Form Name">
                </div>



            }
        </form>

    </div>
</div>
@section Scripts {
    @{
        @Scripts.Render("~/bundles/jqueryval")
        <script src="~/Content/vendor/jquery/jquery.min.js"></script>
        <script src="~/Content/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

        <!-- Core plugin JavaScript-->
        <script src="~/Content/vendor/jquery-easing/jquery.easing.min.js"></script>

        <!-- Custom scripts for all pages-->
        <script src="~/Content/js/sb-admin-2.min.js"></script>

        <!-- Page level plugins -->
        <script src="~/Content/vendor/chart.js/Chart.min.js"></script>

        <!-- Page level custom scripts -->
        <script src="~/Content/js/demo/chart-area-demo.js"></script>
        <script src="~/Content/js/demo/chart-pie-demo.js"></script>

    }
}