@{
    ViewData["Title"] = "Home Page";
}
@Styles.Render("~/content/assets/css/templatemo-space-dynamic.css")



<div class="main-banner wow fadeIn" id="top" data-wow-duration="1s" data-wow-delay="0.5s">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="row">
                    <div class="col-lg-6 align-self-center">
                        <div class="left-content header-text wow fadeInLeft" data-wow-duration="1s" data-wow-delay="1s">
                            <h6>//customize it in views/home/index it just demo for test//</h6>
                            <h6>Welcome to LasFlores</h6>
                            <h2>We Make <em>Digital Ideas</em> &amp; <span>SEO</span> Marketing</h2>
                            <p>LasFlores is a professional looking HTML template using a Bootstrap 5 (beta 2). This CSS template is free for you provided by .</p>

                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="right-image wow fadeInRight" data-wow-duration="1s" data-wow-delay="0.5s">
                            <img src="~/content/images/logo.jpeg" alt="team meeting" style="width:100% !important">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    @{
        <script src="~/content/assets/js/templatemo-custom.js"></script>
        <script src="~/content/assets/js/owl-carousel.js"></script>
        <script src="~/content/assets/js/animation.js"></script>
        <script src="~/content/assets/js/imagesloaded.js"></script>

    }
}
